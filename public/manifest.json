{
  "manifest_version": 3,
  "name": "Dotab",
  "version": "0.1.0",
  "description": "一个新标签页扩展，由Vue 3强力驱动！",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "favicon"],
  "host_permissions": ["*://*.google.com/s2/favicons*", "*://*.gstatic.com/faviconV2*"]
}
